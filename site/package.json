{
    "name": "site",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build:dll": "rimraf dist && mkdirp dist && webpack --hide-modules --config config/webpack.dll.config.babel.js",
        "dev-server": "cross-env NODE_ENV=development PORT=8000 node -r babel-register config/dev-server.js",
        "build": "cross-env NODE_ENV=production webpack --hide-modules --config config/webpack.config.babel.js",
        "build:dev": "cross-env NODE_ENV=development PORT=8000 webpack --hide-modules --config config/webpack.config.babel.js",
        "dev": "cross-env NODE_ENV=development npm run build:dll && npm run build:dev && npm run dev-server"
    },
    "dependencies": {
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1"
    },
    "devDependencies": {
        "assets-webpack-plugin": "^3.5.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "babel-runtime": "^6.23.0",
        "classnames": "^2.2.5",
        "connect-history-api-fallback": "^1.3.0",
        "cross-env": "^5.0.1",
        "express": "^4.15.3",
        "extract-text-webpack-plugin": "^3.0.0",
        "html-webpack-plugin": "^2.29.0",
        "json-loader": "^0.5.4",
        "less": "^2.7.2",
        "less-loader": "^4.0.5",
        "mkdirp": "^0.5.1",
        "optimize-css-assets-webpack-plugin": "^2.0.0",
        "react-hot-loader": "^1.3.1",
        "rimraf": "^2.6.1",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.3.0",
        "webpack-bundle-analyzer": "^2.8.3",
        "webpack-bundle-size-analyzer": "^2.7.0",
        "webpack-dev-middleware": "^1.11.0",
        "webpack-hot-middleware": "^2.18.2",
        "webpackstablechunkid": "^0.0.1"
    }
}
